import { supabase } from '@/integrations/supabase/client';
import { Tables } from '@/integrations/supabase/types';

export type Event = Tables<'events'> & {
  requirements: string[];
  includes: string[];
  agenda: { time: string; activity: string }[];
  flyer: {
    url: string;
    downloadUrl: string;
    alt: string;
  };
};

export type Registration = Tables<'registrations'>;
export type Waitlist = Tables<'waitlist'>;
export type EventFeedback = Tables<'event_feedback'>;

export async function getEvents() {
  try {
    const { data, error } = await supabase
      .from('events')
      .select('*')
      .order('date', { ascending: true });
      
    if (error) {
      console.error('Error fetching events:', error);
      return [];
    }
    
    return data.map(event => ({
      ...event,
      requirements: event.requirements ? JSON.parse(event.requirements as string) : [],
      includes: event.includes ? JSON.parse(event.includes as string) : [],
      agenda: event.agenda ? JSON.parse(event.agenda as string) : [],
      flyer: event.flyer ? JSON.parse(event.flyer as string) : {
        url: "/api/placeholder/600/800",
        downloadUrl: `/downloads/${event.title.toLowerCase().replace(/\s+/g, '-')}-flyer.pdf`,
        alt: `${event.title} Event Flyer`
      }
    })) as Event[];
  } catch (error) {
    console.error('Error processing events:', error);
    return [];
  }
}

export async function getEventById(id: string) {
  try {
    const { data, error } = await supabase
      .from('events')
      .select('*')
      .eq('id', id)
      .single();
      
    if (error) {
      console.error(`Error fetching event with ID ${id}:`, error);
      return null;
    }
    
    if (!data) {
      return null;
    }
    
    return {
      ...data,
      requirements: data.requirements ? JSON.parse(data.requirements as string) : [],
      includes: data.includes ? JSON.parse(data.includes as string) : [],
      agenda: data.agenda ? JSON.parse(data.agenda as string) : [],
      flyer: data.flyer ? JSON.parse(data.flyer as string) : {
        url: "/api/placeholder/600/800",
        downloadUrl: `/downloads/${data.title.toLowerCase().replace(/\s+/g, '-')}-flyer.pdf`,
        alt: `${data.title} Event Flyer`
      }
    } as Event;
  } catch (error) {
    console.error(`Error processing event with ID ${id}:`, error);
    return null;
  }
}

export async function registerForEvent(eventId: string, registrationData: {
  name: string;
  email: string;
  phone: string;
  emergencyContact?: string;
  dietaryRequirements?: string;
  additionalInfo?: string;
}) {
  try {
    // Get the current user
    const { data: userData } = await supabase.auth.getUser();
    const userId = userData.user?.id;
    
    // First check if there are spots available
    const { data: eventData, error: eventError } = await supabase
      .from('events')
      .select('spots')
      .eq('id', eventId)
      .single();
      
    if (eventError) {
      console.error(`Error checking event availability:`, eventError);
      return { success: false, error: 'Failed to check event availability' };
    }
    
    if (!eventData || eventData.spots <= 0) {
      // No spots available, add to waitlist instead
      const { data: waitlistData, error: waitlistError } = await supabase
        .from('waitlist')
        .insert({
          event_id: eventId,
          user_id: userId,
          name: registrationData.name,
          email: registrationData.email,
          phone: registrationData.phone,
        });
        
      if (waitlistError) {
        console.error(`Error adding to waitlist:`, waitlistError);
        return { success: false, error: 'Failed to add to waitlist', isWaitlist: true };
      }
      
      return { success: true, isWaitlist: true, data: waitlistData };
    }
    
    // Spots available, proceed with registration
    const { data, error } = await supabase
      .from('registrations')
      .insert({
        event_id: eventId,
        user_id: userId,
        name: registrationData.name,
        email: registrationData.email,
        phone: registrationData.phone,
        emergency_contact: registrationData.emergencyContact,
        dietary_requirements: registrationData.dietaryRequirements,
        additional_info: registrationData.additionalInfo,
      });
      
    if (error) {
      console.error(`Error registering for event:`, error);
      return { success: false, error: 'Failed to register for event' };
    }
    
    return { success: true, data };
  } catch (error) {
    console.error(`Error during registration:`, error);
    return { success: false, error: 'An unexpected error occurred' };
  }
}

export async function getEventRegistrations(eventId: string, status?: string) {
  try {
    let query = supabase
      .from('registrations')
      .select('*')
      .eq('event_id', eventId);
      
    if (status) {
      query = query.eq('status', status);
    }
    
    const { data, error } = await query.order('created_at', { ascending: false });
      
    if (error) {
      console.error(`Error fetching registrations:`, error);
      return [];
    }
    
    return data as Registration[];
  } catch (error) {
    console.error(`Error processing registrations:`, error);
    return [];
  }
}

export async function getUserRegistrations() {
  try {
    const { data: userData } = await supabase.auth.getUser();
    
    if (!userData.user) {
      return [];
    }
    
    const { data, error } = await supabase
      .from('registrations')
      .select(`
        *,
        events (
          id, title, date, time, location, image, category
        )
      `)
      .eq('user_id', userData.user.id)
      .order('created_at', { ascending: false });
      
    if (error) {
      console.error(`Error fetching user registrations:`, error);
      return [];
    }
    
    return data;
  } catch (error) {
    console.error(`Error processing user registrations:`, error);
    return [];
  }
}

export async function submitEventFeedback(registrationId: string, feedbackData: {
  rating: number;
  feedbackText?: string;
  isAnonymous?: boolean;
}) {
  try {
    // Get event_id from registration
    const { data: registrationData, error: registrationError } = await supabase
      .from('registrations')
      .select('event_id, user_id')
      .eq('id', registrationId)
      .single();
      
    if (registrationError) {
      console.error(`Error fetching registration:`, registrationError);
      return { success: false, error: 'Failed to fetch registration details' };
    }
    
    const { data, error } = await supabase
      .from('event_feedback')
      .insert([{
        event_id: registrationData.event_id,
        user_id: registrationData.user_id,
        registration_id: registrationId,
        rating: feedbackData.rating,
        feedback_text: feedbackData.feedbackText,
        is_anonymous: feedbackData.isAnonymous || false
      }]);
      
    if (error) {
      console.error(`Error submitting feedback:`, error);
      return { success: false, error: 'Failed to submit feedback' };
    }
    
    return { success: true, data };
  } catch (error) {
    console.error(`Error during feedback submission:`, error);
    return { success: false, error: 'An unexpected error occurred' };
  }
}

