# Public environment variables for Vite (prefix VITE_)
# Copy this file to .env (local) or set as build env vars in your hosting platform.

# Supabase
VITE_SUPABASE_URL=
VITE_SUPABASE_ANON_KEY=

# Cloudinary (image uploads)
VITE_CLOUDINARY_CLOUD_NAME=
VITE_CLOUDINARY_UPLOAD_PRESET=
VITE_CLOUDINARY_FOLDER=flyers
# Optional gallery-specific overrides
VITE_CLOUDINARY_GALLERY_UPLOAD_PRESET=
VITE_CLOUDINARY_GALLERY_FOLDER=

# EmailJS
VITE_EMAILJS_SERVICE_ID=
VITE_EMAILJS_PUBLIC_KEY=
VITE_EMAILJS_REGISTRATION_TEMPLATE=template_event_registration
VITE_EMAILJS_ADMIN_NOTIFICATION_TEMPLATE=template_admin_notification
VITE_EMAILJS_CONTACT_TEMPLATE=template_contact_form
# Optional additional templates
VITE_EMAILJS_REMINDER_TEMPLATE=template_event_reminder
VITE_EMAILJS_CANCELLATION_TEMPLATE=template_event_cancellation

# Organization
VITE_ORGANIZER_EMAIL=events@gamesandconnect.com
VITE_ORGANIZATION_NAME=Games & Connect
